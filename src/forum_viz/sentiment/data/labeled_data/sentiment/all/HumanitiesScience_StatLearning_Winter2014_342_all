aS'require(leaps)'
p191
aS'hitters=na.omit(hitters)'
p192
aS'predict.regsubsets=function(object,newdata,id,...){'
p193
aS'form=as.formula(object$call[[2]])'
p194
aS'mat=model.matrix(form,newdata)'
p195
aS'coefi=coef(object,id=id)'
p196
aS'mat[,names(coefi)]%*%coefi'
p197
aS'}'
p198
aS'set.seed(11)'
p199
aS'folds=sample(rep(1:10,length=nrow(hitters)))'
p200
aS'folds'
p201
aS'table(folds)'
p202
aS'cv.errors=matrix(na,10,19)'
p203
aS'for(k'
p204
aS'in'
p205
aS'1:10){'
p206
aS'best.fit=regsubsets(salary~.,data=hitters[folds!=k,],nvmax=19,method="forward")'
p207
aS'for(i'
p208
aS'in'
p209
aS'1:19){'
p210
aS'pred=predict(best.fit,hitters[folds==k,],id=i)'
p211
aS'cv.errors[k,i]=mean('
p212
aS'(hitters$salary[folds==k]-pred)^2)'
p213
aS'}'
p214
aS'}'
p215
aS'rmse.cv=sqrt(apply(cv.errors,2,mean))'
p216
